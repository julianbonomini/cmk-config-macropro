#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp KP_N3   &mo 1
                &kp KP_N1   &kp KP_N2
            >;
        };

        bt_layer {
            bindings = <
                &none   &none
                &bt BT_CLR   &bootloader
            >;
        };
    };
};